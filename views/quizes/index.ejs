<form id='searchForm' method='get' action='/quizes'>
	<input name='search' type='text' placeholder='Texto a buscar...'/>
	<input type='submit' value='Buscar'/>
</form>
<table>
	<% for (var i = 0; i < quizes.length; i++) { %>
		<tr>
			<td><a href='quizes/<%= quizes[i].id %>'><%= quizes[i].pregunta %></a></td>
		</tr>
	<% } %>
</table>
